<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
                  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:prism="clr-namespace:Prism.Mvvm;assembly=Prism.Forms"
                  prism:ViewModelLocator.AutowireViewModel="True"
                  x:Class="PrismUnityAppBase.Views.PrismMasterDetailPage">
    <MasterDetailPage.Master>
        <ContentPage Title="Default">
            <StackLayout>
                <Button Text="MainPage" Command="{Binding NavigateCommand}" CommandParameter="PrismNavigationPage/MainPage" />
                <Button Text="ViewA" Command="{Binding NavigateCommand}" CommandParameter="PrismNavigationPage/ViewA" />
                <Button Text="ViewB" Command="{Binding NavigateCommand}" CommandParameter="PrismNavigationPage/ViewB" />
                <Button Text="ViewC" Command="{Binding NavigateCommand}" CommandParameter="PrismNavigationPage/ViewC" />
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>
</MasterDetailPage>